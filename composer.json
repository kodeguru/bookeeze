{
    "name": "kartik-v/yii2-app-practical",
    "description": "Yii 2 Practical Application Template (based on advanced)",
    "keywords": ["yii2", "framework", "practical", "application template"],
    "homepage": "http://demos.krajee.com/app-practical",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/kartik-v/yii2-app-practical/issues?state=open",
        "source": "https://github.com/kartik-v/yii2-app-practical/"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-tree-manager": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "2amigos/yii2-date-picker-widget": "@dev",
        "kartik-v/yii2-detail-view": "@dev",
        "kartik-v/yii2-builder": "@dev",
        "kartik-v/yii2-date-range": "*",
        "mdmsoft/yii2-admin": "~2.0",
        "rmrevin/yii2-fontawesome": "~2.17",
        "kartik-v/yii2-tabs-x": "*",
        "yiidoc/yii2-redactor": "*",
        "philippfrenzel/yii2fullcalendar": "*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "yiisoft/yii2-jui": "^2.0",
        "2amigos/yii2-qrcode-helper": "~1.0",
        "yii2mod/yii2-google-maps-markers": "*",
        "yiisoft/yii2-imagine": "^2.0@dev",
        "kalyabin/yii2-select-google-map-location": "*",
        "kartik-v/yii2-widget-rating": "*",
        "yii2mod/yii2-star-rating": "dev-master",
        "vendor/yii2mod/yii2-star-rating": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
